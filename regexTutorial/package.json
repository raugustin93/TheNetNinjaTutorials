  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "browser-sync": "^2.27.10",
    "concurrently": "^4.1.0",
    "pug": "^2.0.3",
    "sass": "^1.19.0"
  }
